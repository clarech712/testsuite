module --force purge 2>/dev/null
module load ${TESTPILOT_COMPILER} > /dev/null 2>&1
module load ${TESTPILOT_MPI} > /dev/null 2>&1
module load ${TESTPILOT_PYTHON} > /dev/null 2>&1
module load ${TESTPILOT_MPI4PY} > /dev/null 2>&1
