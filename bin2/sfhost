source /cluster/tufts/hpc/swtests/testapps_python_login-prod-03_114355/test_source.module

cd ${SLURM_SUBMIT_DIR:-.}
python matrix.py
date +%s > /cluster/tufts/hpc/swtests/testapps_python_login-prod-03_114355/timestamp2
