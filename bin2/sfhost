#!/bin/bash
#SBATCH -o testpilot-python.o%j
#SBATCH -e testpilot-python.e%j
#!/bin/bash
#SBATCH -N1 -t5
echo $SLURM_JOB_NODELIST | tr '\n' ' '; echo ""
date +%s > /cluster/tufts/hpc/swtests/testapps_python_login-prod-03_114355/timestamp
